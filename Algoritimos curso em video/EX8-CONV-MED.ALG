Algoritmo "Conv-med-ex8"


Var
km,hm,dam,m,dm,cm,mm: real

Inicio
escreva ("Digite uma distancia em metros.")
leia (m)
dm<- m*10
cm<-dm*10
mm<-cm*10
dam<-m/10
hm<-dam/10
km<-dam/10

escreval ("A distância de ",m,"m corresponde a:")
escreval (km,"km")
escreval (hm,"hm")
escreval (dam,"dam")
escreval (m,"m")
escreval (dm,"dm")
escreval (cm,"cm")
escreval (mm,"mm")


Fimalgoritmo